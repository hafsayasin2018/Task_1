        "check_prime = int(input(\"Enter the number\"))\n",
        "def is_prime(check_prime):\n",
        "  if check_prime > 1:\n",
        "      return -1\n",
        "\n",
        "  for i in range(2,(check_prime)):\n",
        "      if (check_prime%i) == 0:\n",
        "        return -1\n",
        "\n",
        "    \n",
        "  return sum(range(check_prime+1))\n",
        "is_prime(check_prime)\n",
        "\n",
        "\n"
